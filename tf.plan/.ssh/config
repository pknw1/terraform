Host github.com
        User git
        Hostname github.com
        PreferredAuthentications publickey
        IdentityFile ~/.ssh/gitlab_io

Host gitlab.com
        User git
        Hostname gitlab.com
        PreferredAuthentications publickey
        IdentityFile /root/.ssh/gitlab_io

Host ca
        User root
        Hostname ca.pknw1.co.uk
        PreferredAuthentications publickey
        IdentityFile ~/.ssh/ns515993
        ForwardX11 yes

Host fr
        User root
        Hostname fr.pknw1.co.uk
        PreferredAuthentications publickey
        IdentityFile ~/.ssh/id_rsa
        ForwardX11 yes


